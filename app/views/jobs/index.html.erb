<h1>Jobs#index</h1>
<p>Find me in app/views/jobs</p>





<div class="container">
  <div class="row justify-content-center">
    <div class="col-sm-8">
      <div id="jobs">
        <% @jobs.each do |jobs| %>
          <h4><%= jobs.title %></h4>
          <p><%= jobs.description %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>